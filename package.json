{
  "name": "xyz.dataparty.rfparty",
  "displayName": "rfparty",
  "version": "1.0.0",
  "description": "A bluetooth tricorder",
  "source": "./index.html",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "parcel index.html --out-dir ./.dist",
    "init": "node ./scripts/init.js",
    "lint": "eslint --fix src/ || exit 0",
    "build-dev": "parcel build index.html --no-minify --no-source-maps --no-content-hash --public-url . --dist-dir ./www index.html",
    "build": "parcel build index.html --no-source-maps --no-content-hash --public-url . --dist-dir ./www index.html"
  },
  "author": "Dataparty",
  "license": "Apache-2.0",
  "devDependencies": {
    "buffer": "^6.0.3",
    "cordova": "11.0.0",
    "cordova-android": "^10.1.2",
    "cordova-plugin-android-permissions": "^1.1.3",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-badge": "^0.8.8",
    "cordova-plugin-console": "^1.1.0",
    "cordova-plugin-device": "^2.1.0",
    "cordova-plugin-local-notification-fixed": "^0.11.0",
    "cordova-plugin-play-default-sounds": "^1.0.1",
    "eslint": "8.19.0",
    "events": "^3.3.0",
    "nodejs-mobile-cordova": "^0.4.3",
    "parcel": "2.6.2",
    "parcel-plugin-clean-dist": "0.0.6",
    "parcel-plugin-static-files-copy": "2.6.0",
    "parcel-resolver-ignore": "^2.1.3",
    "parcel-transformer-google-fonts": "^0.1.4",
    "stream-browserify": "^3.0.0"
  },
  "dependencies": {
    "cordova-plugin-whitelist": "1.3.5",
    "csv": "^6.1.5",
    "debug": "^4.3.4",
    "json-viewer-js": "^1.0.8",
    "json5": "^2.2.1",
    "jsonpath-plus": "^7.0.0",
    "last-eventemitter": "^1.1.1",
    "leaflet": "^1.8.0",
    "lodash": "^4.17.21",
    "lokijs": "^1.5.12",
    "moment": "^2.29.3",
    "moment-timezone": "^0.5.34",
    "xml-js": "^1.6.11"
  },
  "staticFiles": {
    "staticPath": [
      "src/assets"
    ],
    "watcherGlob": "**"
  },
  "parcelIgnore": [
    "cordova.js"
  ],
  "transforms": {
    "*.html": [
      "parcel-transformer-google-fonts"
    ]
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "nodejs-mobile-cordova": {},
      "cordova-plugin-console": {},
      "cordova-plugin-android-permissions": {},
      "cordova-plugin-androidx-adapter": {},
      "cordova-plugin-badge": {},
      "cordova-plugin-local-notification-fixed": {
        "ANDROID_SUPPORT_V4_VERSION": "26.+"
      },
      "cordova-plugin-play-default-sounds": {}
    },
    "platforms": []
  }
}